<?xml version="1.0" encoding="utf-8"?><androidx.constraintlayout.widget.ConstraintLayout        xmlns:android="http://schemas.android.com/apk/res/android"        xmlns:app="http://schemas.android.com/apk/res-auto"        xmlns:tools="http://schemas.android.com/tools"        android:layout_width="match_parent"        android:layout_height="wrap_content"        android:paddingTop="8dp"        android:paddingEnd="16dp"        android:paddingBottom="8dp"        android:paddingLeft="16dp"        android:id="@+id/linearLayout"        android:background="@drawable/ripple">    <ImageView            android:layout_width="24dp"            android:layout_height="24dp"            app:srcCompat="@mipmap/ic_launcher_round"            app:layout_constraintTop_toTopOf="parent"            android:id="@+id/iconRecyclerDialog"            app:layout_constraintStart_toStartOf="parent"            android:layout_marginTop="8dp"            android:visibility="gone"            android:layout_marginBottom="8dp"            android:layout_marginLeft="16dp"            app:layout_constraintBottom_toBottomOf="parent"    />    <TextView            android:text="@string/title_dialog"            android:layout_width="0dp"            android:layout_height="wrap_content"            app:layout_constraintTop_toTopOf="parent"            android:id="@+id/titleRecyclerDialog"            app:layout_constraintBottom_toBottomOf="parent"            app:layout_constraintStart_toEndOf="@+id/iconRecyclerDialog"            app:layout_constraintEnd_toEndOf="parent"            android:textColor="@color/black"            android:textSize="18sp"    /></androidx.constraintlayout.widget.ConstraintLayout>