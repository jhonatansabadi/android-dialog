<?xml version="1.0" encoding="utf-8"?>

<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        xmlns:tools="http://schemas.android.com/tools"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" android:id="@+id/linearLayout">
    <ImageView
            android:layout_width="32dp"
            android:layout_height="32dp" app:srcCompat="@mipmap/ic_launcher_round"
            app:layout_constraintTop_toTopOf="parent"
            android:id="@+id/imageView2" app:layout_constraintStart_toStartOf="parent"
    />
    <TextView
            android:text="TextView"
            android:layout_width="0dp"
            android:layout_height="wrap_content" app:layout_constraintTop_toTopOf="@+id/imageView2"
            android:id="@+id/titleMultItem"
            app:layout_constraintBottom_toBottomOf="@+id/imageView2"
            app:layout_constraintStart_toEndOf="@+id/imageView2" android:layout_marginStart="16dp"
            app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="8dp" android:textColor="#000000"
            android:textSize="18sp"/>
</androidx.constraintlayout.widget.ConstraintLayout>