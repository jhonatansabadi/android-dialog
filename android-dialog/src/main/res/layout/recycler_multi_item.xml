<?xml version="1.0" encoding="utf-8"?><com.google.android.material.card.MaterialCardView xmlns:android="http://schemas.android.com/apk/res/android"    xmlns:app="http://schemas.android.com/apk/res-auto"    android:layout_width="match_parent"    android:layout_height="wrap_content"    xmlns:tools="http://schemas.android.com/tools"    android:paddingLeft="@dimen/dimen_margin_medium"    android:paddingTop="@dimen/dimen_margin_min"    android:paddingEnd="@dimen/dimen_margin_medium"    android:paddingBottom="@dimen/dimen_margin_min"    app:cardBackgroundColor="?android:attr/colorBackground">    <androidx.constraintlayout.widget.ConstraintLayout        android:id="@+id/linearLayout"        android:layout_width="match_parent"        android:layout_height="wrap_content"        android:background="?android:attr/colorBackground">        <ImageView            android:id="@+id/iconRecyclerDialog"            android:layout_width="@dimen/dimen_icon_size_medium"            android:layout_height="@dimen/dimen_icon_size_medium"            android:layout_marginLeft="@dimen/dimen_margin_medium"            android:visibility="gone"            tools:visibility="visible"            app:layout_constraintBottom_toBottomOf="parent"            app:layout_constraintStart_toStartOf="parent"            app:layout_constraintTop_toTopOf="parent"            app:srcCompat="@drawable/ic_directions_walk_black_24dp" />        <TextView            android:id="@+id/titleRecyclerDialog"            android:layout_width="0dp"            android:layout_height="wrap_content"            android:layout_marginStart="@dimen/dimen_margin_detault"            android:layout_marginTop="@dimen/dimen_margin_min"            android:layout_marginBottom="@dimen/dimen_margin_min"            tools:text="@string/title_dialog"            android:textColor="?android:attr/textColorPrimary"            android:textSize="@dimen/dimen_text_large"            app:layout_constraintBottom_toBottomOf="parent"            app:layout_constraintEnd_toEndOf="parent"            app:layout_constraintStart_toEndOf="@+id/iconRecyclerDialog"            app:layout_constraintTop_toTopOf="parent" />    </androidx.constraintlayout.widget.ConstraintLayout></com.google.android.material.card.MaterialCardView>